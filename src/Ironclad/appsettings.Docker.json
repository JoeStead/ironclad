{
  "urls": "http://*:80;",
  "serilog": {
    "minimumLevel": {
      "default": "Information"
    },
    "writeTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "RollingFile",
              "Args": { "pathFormat": "logs/ironclad/ironclad-docker-{Date}.log" }
            }
          ]
        }
      }
    ]
  }
}